@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Search;
@inherits SearchViewBase
@section conditionParam{
    <row>
        <i-col span="2" class="RowTitle">物料代码</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.GOODSDM",
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">物料名称</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.NAME",
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">物料供货商</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.MERCHANTID"
                });
            }
        </i-col>
    </row>
}
