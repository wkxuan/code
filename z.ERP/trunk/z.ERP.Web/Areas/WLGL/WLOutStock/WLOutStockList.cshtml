@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Search;
@inherits SearchViewBase
@section conditionParam{
    <row>
        <i-col span="2" class="RowTitle">供货商代码</i-col>
        <i-col span="6"  class="RowText">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.MERCHANTID",
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">供货商名称</i-col>
        <i-col span="6" class="RowText">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.NAME",
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            登记人
        </i-col>
        <i-col span="6" class="RowText">
            <i-input readonly icon="md-search" v-model="searchParam.REPORTER_NAME" v-on:on-click="SelReporter" />
        </i-col>
        <i-col span="2" class="RowTitle">
            审核人
        </i-col>
        <i-col span="6" class="RowText">
            <i-input readonly icon="md-search" v-model="searchParam.VERIFY_NAME" v-on:on-click="SelVerify" />
        </i-col>
    </row>
}
