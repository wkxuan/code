@inherits ViewBase<dynamic>
@using z.ERP.Web.Areas.Share.Render
@using z.MVC5.Views
@{
    Layout = ThisLayout;
}
@{
    WebFiles = new string[] {
                };
}

@section head{
    @InitFiles(WebFiles)
    @RenderSection("head", false)
}
<div id="detail_Main">
    <collapse v-model="Mainpanel">
        <panel name="condition">
            @RenderSection("Title", false)
            <div style="float: right; width: calc(100% - 160px) ;height:100%;text-align: right;line-height: inherit;padding-right: 10px;">
                @RenderSection("Btn", false)
            </div>
            <p slot="content" class="element">
                @RenderSection("content", true)
            </p>
        </panel>
    </collapse>
    @RenderSection("baseParam", false)
</div>
@section foot{
    @RenderSection("foot", false)
}
