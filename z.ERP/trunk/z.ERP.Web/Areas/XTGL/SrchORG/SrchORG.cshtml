@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Srch;
@inherits SrchViewBase
@section conditionParam{
    <row>
        <i-col span="2" class="RowTitle">组织代码</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.ORGCODE"
                });
            }
        </i-col>

        <i-col span="2" class="RowTitle">组织名称</i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender()
                {
                    vModel = "searchParam.ORGNAME"
                });
            }
        </i-col>
    </row>
}