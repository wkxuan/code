@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Web.Areas.Layout.Empty
@inherits EmptyViewBase

@section content{
    <row>
        @{
            RenderControl(new ButtonRender()
            {
                Click = "exec",
                Text = "执行",
                Enable = false
            });
        }
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            日期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new DateBoxRender()
                {
                    vModel = "RQ",
                    Enable = false
                });
            }
        </i-col>
    </row>
}
