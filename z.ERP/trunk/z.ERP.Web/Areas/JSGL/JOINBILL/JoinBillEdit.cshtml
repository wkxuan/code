@using z.ERP.Web.Areas.Share.Render
@using z.ERP.Entities.Enum
@using z.ERP.Web.Areas.Layout.Edit;
@using z.ERP.Entities;
@inherits EditViewBase<string>

@section baseParam{    
    <row>
        <i-col span="2" class="RowTitle">
            合同号
        </i-col>
        <i-col span="6" class="RowText">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.CONTRACTID",
                    Id = "tb_cont",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">
            商户
        </i-col>
        <i-col span="2">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.MERCHANTID",
                    Id = "tb_merch",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="4">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.MERCHANTNAME",
                    Id = "tb_merch",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">
            类型
        </i-col>
        <i-col span="2" class="RowText">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.TYPE",
                    Id = "tb_type",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">
            状态
        </i-col>
        <i-col span="2" class="RowText">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.STATUS",
                    Id = "tb_status",
                    Readonly = "true"
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            年月
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.NIANYUE",
                    Id = "tb_ny",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">
            开始日期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.START_DATE",
                    Id = "tb_ksrq",
                    Readonly = "true"
                });
            }
        </i-col>
        <i-col span="2" class="RowTitle">
            结束日期
        </i-col>
        <i-col span="6">
            @{
                RenderControl(new TextBoxRender
                {
                    vModel = "dataParam.END_DATE",
                    Id = "tb_jsrq",
                    Readonly = "true"
                });
            }
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            
        </i-col>
        <i-col span="2" class="RowTitle">
           价款
        </i-col>
        <i-col span="2" class="RowTitle">
           税金
        </i-col>
        <i-col span="2" class="RowTitle">
            价税合计
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            16%税率:
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JE_16}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.ZZSJE_16}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JSHJ_16}}
        </i-col>
        <i-col span="2" class="RowTitle">

        </i-col>
        <i-col span="2" class="RowTitle">
            价款：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JKHJ}}
        </i-col>
        <i-col span="2" class="RowTitle">
            税金：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.ZZSJEHJ}}
        </i-col>
        <i-col span="2" class="RowTitle">
            价税合计：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JSHJ}}
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            10%税率:
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JE_10}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.ZZSJE_10}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JSHJ_10}}
        </i-col>
        <i-col span="2" class="RowTitle">

        </i-col>
        <i-col span="2" class="RowTitle">
            销售金额：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.SELL_JE}}
        </i-col>
        <i-col span="2" class="RowTitle">
            销售成本：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.SELL_COST}}
        </i-col>
        <i-col span="2" class="RowTitle">
            扣款金额：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.KKJE}}
        </i-col>
    </row>
    <row>
        <i-col span="2" class="RowTitle">
            其他税率:
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JE_QT}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.ZZSJE_QT}}
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.JSHJ_QT}}
        </i-col>
        <i-col span="2" class="RowTitle">

        </i-col>
        <i-col span="2" class="RowTitle">
            实际付款金额：
        </i-col>
        <i-col span="2" class="RowTitle">
            {{dataParam.SJFKJE}}
        </i-col>
    </row>

}

@section detailParam{
    <row>
        <i-col span="2" class="RowTitle">
            结算单商品明细
        </i-col>
        <i-col span="14" class="RowTitle">
            
        </i-col>
        <i-col span="2" class="RowTitle">
            结算单扣款明细
        </i-col>
    </row>
    <row>
        <i-col span="16" class="RowTitle">
            <i-table size="small" :border="true"
             :columns="screenParam.goodscolDef" :data="dataParam.BILL_GOODS"></i-table>
        </i-col>
        <i-col span="8" class="RowTitle">
            <i-table size="small" :border="true"
             :columns="screenParam.trimcolDef" :data="dataParam.BILL_TRIM"></i-table>
         </i-col>
    </row>
}